{
    "name": "back",
    "version": "1.0.0",
    "scripts": {
        "start": "node ./index.js",
        "dev": "webpack --config webpack.js --mode development --watch",
        "test": "jest -c ./jest/config.unit.js --forceExit",
        "test:watch": "jest -c ./jest/config.unit.js --watch",
        "test:debug": "node --inspect-brk=0.0.0.0:4001 -r tsconfig-paths/register -r ./node_modules/.bin/jest -c ./jest/config.unit.js  --runInBand",
        "test:int": "jest -c ./jest/config.int.js --runInBand --forceExit --passWithNoTests",
        "test:int:watch": "jest -c ./jest/config.int.js --runInBand --watch",
        "build": "rm -rf ./build && webpack --config webpack.js --mode production",
        "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migrate:local": "./script/migrate.sh local",
        "lint": "eslint ./src/ --ext .js,.ts",
        "lint:fix": "eslint ./src/ --fix --ext .js,.ts",
        "techdebt": "leasot --exit-nicely --reporter table --skip-unsupported --ignore node_modules,build,migrations ./",
        "image:build": "./script/image.build.sh",
        "image:push": "./script/image.push.sh"
    },
    "keywords": [],
    "author": "gannochenko.sv@gmail.com",
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@babel/polyfill": "^7.7.0",
        "@bucket-of-bolts/express-mvc": "^1.0.17",
        "@bucket-of-bolts/microdash": "^1.0.14",
        "@bucket-of-bolts/util": "^1.0.14",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "helmet": "^3.21.2",
        "moment": "^2.24.0",
        "pg": "^7.14.0",
        "typeorm": "^0.2.20"
    },
    "devDependencies": {
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-typescript": "^7.7.4",
        "@types/cors": "^2.8.6",
        "@types/helmet": "^0.0.45",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.14",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "copy-webpack-plugin": "^5.0.5",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.1",
        "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "leasot": "^9.3.0",
        "nodemon": "^2.0.1",
        "nodemon-webpack-plugin": "^4.2.1",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "supertest": "^4.0.2",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
